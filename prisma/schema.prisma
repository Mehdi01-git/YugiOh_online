model YuGiOhDB {
  CardName      String? @db.VarChar(256)
  id            Int @id
  CardType      String? @db.VarChar
  Attribute     String? @db.VarChar
  Property      String? @db.VarChar
  Types         String? @db.VarChar
  Level         Int?
  ATK           Int?
  DEF           Int?
  Link          Int?
